{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc)?$"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "logo": {
      "url": "./logo.png",
      "width": "140px",
      "height": "50px",
      "link": "https://www.npmjs.com/package/filemanager-plugin"
    },
    "useCollapsibles": true,
    "name": "filemanager-plugin",
    "footerText": "filemanager-plugin api doc",
    "default": {
      "staticFiles": {
        "include": ["./styles/logo.png", "./styles/custom.css"]
      }
    },
    "css": ["custom.css"]
  },
  "opts": {
    "template": "node_modules/tui-jsdoc-template",
    "destination": "./doc",
    "recurse": true
  }
}
