{
  "name": "filemanager-plugin",
  "version": "2.0.0",
  "description": "This is file manager plugin.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xianweics/filemanager-plugin.git"
  },
  "keywords": [
    "webpack",
    "rollup",
    "file manager",
    "nodejs",
    "mocha"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xianweics/filemanager-plugin/issues"
  },
  "homepage": "https://github.com/xianweics/filemanager-plugin#readme",
  "scripts": {
    "start": "node_modules/.bin/rollup --config rollup.config.js  --watch",
    "build": "node_modules/.bin/rollup --config rollup.config.js",
    "test": "node_modules/.bin/mocha --config .mocharc.json",
    "test:watch": "node_modules/.bin/mocha --config .mocharc.json --watch",
    "test:coverage": "node_modules/.bin/nyc npm test",
    "test:coveralls": "npm run test:coverage && nyc report --reporter=text-lcov | coveralls",
    "doc": "node_modules/.bin/jsdoc -c ./conf.json",
    "prettier-eslint": "prettier-eslint src/**/*.js  --write",
    "format": "npm run prettier-eslint",
    "commit:check": "npm run format && npm run test"
  },
  "pre-commit": [
    "commit:check"
  ],
  "dependencies": {
    "colors": "^1.4.0",
    "compressing": "^1.5.0",
    "fs-extra": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/register": "^7.8.6",
    "@babel/runtime": "^7.8.4",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.6.0",
    "eslint-friendly-formatter": "^4.0.1",
    "glob": "^7.1.6",
    "jsdoc": "^3.6.3",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "pre-commit": "^1.2.2",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-eslint": "^7.0.0",
    "tui-jsdoc-template": "^1.2.2"
  }
}
